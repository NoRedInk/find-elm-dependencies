{
  "name": "find-elm-dependencies",
  "version": "2.0.4",
  "description": "Find elm dependencies",
  "main": "dist/index.js",
  "scripts": {
    "prepack": "npm run build",
    "build": "rm -rf dist/ && tsc",
    "test": "mocha test/**/*.js --require ts-node/register --watch-extensions ts",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "directories": {
    "test": "tests",
    "bin": "bin",
    "lib": "src"
  },
  "bin": {
    "find-elm-dependencies": "dist/bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noredink/find-elm-dependencies.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "elm",
    "dependencies",
    "node"
  ],
  "author": "Richard Feldman",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/noredink/find-elm-dependencies/issues"
  },
  "homepage": "https://github.com/noredink/find-elm-dependencies",
  "dependencies": {
    "firstline": "^1.2.0",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "mocha": "^8.1.1",
    "snyk": "^1.316.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "snyk": true
}